{
  "name": "bosnia-lens",
  "private": true,
  "scripts": {
    "db:deploy_generate": "cd backend && npx prisma migrate deploy && npx prisma generate",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd backend && npm run test",
    "test:all": "concurrently \"npm run test:backend\" \"npm run test:frontend\"",
    "test:coverage:backend": "cd backend && npx vitest --coverage",
    "test:coverage:frontend": "cd frontend && npx vitest --coverage",
    "test:coverage:all": "concurrently \"cd backend && npx vitest --coverage\" \"cd frontend && npx vitest --coverage\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goran1010/bosnia-lens.git"
  },
  "keywords": [],
  "author": "Goran JoviÄ‡",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/goran1010/bosnia-lens/issues"
  },
  "homepage": "https://github.com/goran1010/bosnia-lens#readme",
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
